/* Layout */

.container {
  @include container;
}

/* -------------------------------------------------------------------------*/

/* == Generic == */
body {
    font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", sans-serif;
    font-weight: $base_font_weight;
    font-size: $base-font-size;
    line-height: $base-line-height;
    -webkit-hyphens: auto;
    -moz-hyphens: auto;
    -ms-hyphens: auto;
    -o-hyphens: auto;
    hyphens: auto;
    word-wrap: break-word;
    height: 100%;
}

a { text-decoration: none; }

/* -- Headings -- */

h1 {
    font-size: 2 * $base-font-size;
    line-height: 2 * $base-line-height;
}

h2 {
    font-size:1.5 * $base-font-size; 
    line-height: 1.5 * $base-line-height;
}

h3 {
    font-size: 1.25 * $base-font-size; 
    line-height: 1.25 * $base-line-height;
    font-weight: 500;
}

h4 { 
    font-size: 1.125 * $base-font-size;
    line-height: 1.125 * $base-line-height; 
}

h5, h6 {
    font-size: $base-font-size;
    line-height: $base-line-height;
}

p, ol, ul {
    @include trailer();
    
    &:only-child, &:last-child { margin-bottom: 0; }
}

ul { 
    list-style-type: disc; 
    padding-left: 0em;
}

/* -- Form Elements -- */

label {
    @include padding-leader(.5);
    @include padding-trailer(.5);
    display: inline-block;
}

/*textarea, */
input[type=text], input[type=password], 
input[type=submit], input[type=button], button, .button {
    display: inline;
    height: 35px;
    border: 0;
    padding: rhythm(.3125) 16px;
    margin: 0;
    @include boxsizing(border-box);
}

input[type=text], input[type=password], textarea { width: 100%; }

input[type=radio] { margin-right: 5px; }

select {
    width: 100%;
    margin-top: 0;
    @include trailer();
}

input[type=submit], button, .button {
    color: #fff;
    text-transform: uppercase;
    cursor: pointer;
}
    
fieldset { 
    border: 0; 
    margin: 0;
    padding: 0;
}
    
    /* -- Misc Classes -- */
    .center-div {
        margin:0 auto;
        overflow: auto;
    }
        
        /* Pagination Classes */
        
        .item-pagination {
            clear:both;
            @include padding-trailer(.5);

            li {
                display:inline-block;
                background-color: #fff;
                margin: 0 0 rhythm(.5) 0;
                padding: rhythm(.25) 8px;
                @include box-shadow(rgba(0,0,0,.15) 0 0 10px);
                
                &:empty { display: none; }
            }

            .next { float: right; }
            .previous { float: left; }
        }
        
/* == Header == */

header { 
    padding: 10px 0px;
}

#site-title {
    font-size: 2.5 * $base-font-size;
    line-height: 2 * $base-line-height;
    left: 60px;
    font-weight: 300;
    text-align: center;
    margin: 8;
    @include padding-leader(.5);
    @include padding-trailer(.3);
    
    > #logowrapper {
        position: relative;
        width: 510px;
        left: 0px;
    }
}

#search-container {
    @include padding-trailer(0.1);
    margin-top:52px;
    
    &:after {
        content: "";
        display: block;
        height: 0;
        width: 100px;
        clear: both;
    }

    > #advanced-search-link{
        width: 150px;
    }

    > div{
        margin-left: 10%;
        margin-right: 10%;
        position: relative;
        float: left;
        word-wrap: normal;
        width: 300px;
    }
    
}

#search-form{
    text-align: center;
    position: relative;
    overflow: visible;

    input[type=text] {
        display: inline;
        width: 60%;
        @include adjust-font-size-to(14px);
        float: left;
        box-shadow:0 0 4px rgba(0,0,0,1);
    	border:0 solid #000000;
    	border-radius:3px;
    }
    
    input[type=submit] {
      width: 30%;
      padding: 0;
      position: absolute;
      top: 0;
      left: 60%;
    }
}

#advanced-search {
    width: 10%;
    height: 35px;
    text-indent: -9999px;
    display: block;
    position: absolute;
    padding: 0;
    top: 0;
    left: 90%;
    font-weight: bold;
    line-height: 30px;
    background-color: #4b6569;
    @include has-layout-zoom();
    @include boxsizing(content-box);
    *zoom: expression( this.runtimeStyle['zoom'] = '1', this.innerHTML = '+');
    *text-indent: 0;

    &:hover {
        text-decoration: underline;
        
    }
    
    &:link, &:active, &:visited { color: #fff; }
    
    
    
    &:after {
        content: url('../images/info-icon.png');
        display: block;
        text-indent: 0;
        text-align: center;
        margin-top: -1.75em;
    }
}

#advanced-form {
    display: none;
    background-color: #fff;
    overflow: auto;
    clear: both;
    z-index: 1001;
    position: absolute;
    top: 35px;
    left: 0;
    border: 3px solid #e7e7e7;
    width: 100%;
    padding: rhythm(1) 16px;
    text-align: left;
    @include boxsizing(border-box);
    @include box-shadow(rgba(0,0,0,.25) 0 5px 10px);
    
    #query-types *:first-child {
        margin-top: 0;
    }
    
    label {
        margin: 0;
        padding: 0;
    }
    
    p {
        @include leader(.5);
        @include trailer(.5);
    }
}

/* == Footer == */
footer {
    @include leader();
    padding: 0 16px;

    
    p {
        @include padding-trailer();
    }
    
    &:empty { display: none; }
}

/* == Content == */

    /* -- Structure -- */
    #content {
        clear:both;
        overflow:visible;
        padding: rhythm(0.25) 4px;
    }
    
    /* -- General -- */
    #home {

        #content div {
            
            .item {
                border-top: 1px solid $winter-light-gray;
                overflow: auto;
                
                &:first-of-type { 
                    border-top: 0; 
                    
                    h3 { margin-top: rhythm(.25); }
                }
                
                h3 { 
                    margin-top: rhythm(.5); 
                    padding-left: 16px;
                    padding-right: 16px;
                }
            }

            .items-list {
                margin-bottom: 0;
                .item-description {
                    margin: rhythm(.5) 16px 0;
                }
            }

            h3, h4, h5, h6,
            p, input, textarea {
                margin: rhythm(.5) 0;
            }

            h3 {
                margin-top: 0;
                font-weight: 300;
                text-transform: uppercase;
            }
            
            img { 
                margin: 0 16px rhythm(.5); 
                border: 1px solid $winter-light-gray;
                display: block;
                max-width: 100%;
                height: auto;
                @include boxsizing(border-box);
            }

            .show {
                display: block;
                text-transform: capitalize;
                &:before { content: "("; }
                &:after { content: "...)"; }
            }
            
            .view-items-link {
                margin: 0;
                padding: rhythm(.5) 16px 0;
                border-top: 1px solid $winter-light-gray;
            }
            
            .item-img {
                display: inline;
            }
            
            .item-description {
                margin-top: 0;
                padding-top: 0;
            }

        }
        
        #intro {
            @include trailer();
        }
        
        #featured-item p { clear: both; }
        
        #recent-items h2 { @include padding-trailer(.5); }

    }
        
    /* -- Navigation -- */
    nav {
        .active { font-weight: 500; }
        
        ul {
            padding-left: 0;
            list-style:none;
            margin:0 auto;
        }
        
        li {
            display:inline-block;
            font-family: 'Open Sans',sans-serif;
/*            font-family: Georgia, Times, "Times New Roman", serif;*/
/*            font-style: italic;*/
            margin-right: 16px;
            ul li {
                padding-left: 16px;
            }
        }
        
        a { display: inline-block; }
    }
    
    .top {
        padding: 0 16px;
        
        ul {
            display: none;
            padding: 0;
        }
        
        a {
            @include padding-leader(.5);
            @include padding-trailer(.5);
        }
        
        .mobile {
            margin: rhythm(1) auto;
            width: 100%;
        }
    }
    
    .navigation ul, ul.navigation { padding-left: 0; }
      

/* == Items Pages == */

.item {
    
    h3 { @include trailer(.5); }
    
    #itemfiles {
            
        #item-images > div { 
            width: 22.5%; 
            display: inline-block;
            margin-right: 3%;
            
            &:nth-of-type(4n) { margin-right: 0; }
        }
            
        a {
            border: 1px solid $winter-light-gray;
            display: inline-block;
            zoom: 1;
            *display: inline;
            min-width: 100%;
            overflow: visible;
            margin: 0;
            padding: 0;
            @include boxsizing(border-box);
            
