/* Layout */

.container {
  @include container;
}

/* -------------------------------------------------------------------------*/

/* == Generic == */
body {
    font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", sans-serif;
    font-weight: $base_font_weight;
    font-size: $base-font-size;
    line-height: $base-line-height;
    -webkit-hyphens: auto;
    -moz-hyphens: auto;
    -ms-hyphens: auto;
    -o-hyphens: auto;
    hyphens: auto;
    word-wrap: break-word;
    height: 100%;
}

a { text-decoration: none; }

/* -- Headings -- */

h1 {
    font-size: 2 * $base-font-size;
    line-height: 2 * $base-line-height;
}

h2 {
    font-size:1.5 * $base-font-size; 
    line-height: 1.5 * $base-line-height;
}

h3 {
    font-size: 1.25 * $base-font-size; 
    line-height: 1.25 * $base-line-height;
    font-weight: 500;
}

h4 { 
    font-size: 1.125 * $base-font-size;
    line-height: 1.125 * $base-line-height; 
}

h5, h6 {
    font-size: $base-font-size;
    line-height: $base-line-height;
}

p, ol, ul {
    @include trailer();
    
    &:only-child, &:last-child { margin-bottom: 0; }
}

ul { 
    list-style-type: disc; 
    padding-left: 1em;
}

/* -- Form Elements -- */

label {
    @include padding-leader(.5);
    @include padding-trailer(.5);
    display: inline-block;
}

input[type=text], input[type=password], textarea,
input[type=submit], button, .button {
    height: 35px;
    border: 0;
    padding: rhythm(.3125) 16px;
    margin: 0;
    @include boxsizing(border-box);
}

input[type=text], input[type=password], textarea { width: 100%; }

input[type=radio] { margin-right: 5px; }

select {
    width: 100%;
    margin-top: 0;
    @include trailer();
}

input[type=submit], button, .button {
    color: #fff;
    text-transform: uppercase;
    cursor: pointer;
}
    
fieldset { 
    border: 0; 
    margin: 0;
    padding: 0;
}
    
    /* -- Misc Classes -- */
    .center-div {
        margin:0 auto;
        overflow: auto;
    }
        
        /* Pagination Classes */
        
        .item-pagination {
            clear:both;
            @include padding-trailer(.5);

            li {
                display:inline-block;
                background-color: #fff;
                margin: 0 0 rhythm(.5) 0;
                padding: rhythm(.25) 8px;
                @include box-shadow(rgba(0,0,0,.15) 0 0 10px);
                
                &:empty { display: none; }
            }

            .next { float: right; }
            .previous { float: left; }
        }
        
/* == Header == */

header { 
    padding: 0 16px;

}

#site-title {
    font-size: 2.5 * $base-font-size;
    line-height: 2 * $base-line-height;
    font-weight: 300;
    text-align: center;
    margin: 0;
    @include padding-leader(.5);
    @include padding-trailer(.5);
}

#search-container {
    @include padding-trailer(0.2);
    
    &:after {
        content: "";
        display: block;
        height: 0;
        width: 100px;
        clear: both;
    }
    
    > div{
        margin: 12px;
        position: relative;
    }
    
}

#search-form{
    text-align: center;
    position: relative;
    overflow: visible;

    input[type=text] {
        width: 60%;
        @include adjust-font-size-to(14px);
        float: left;
        
    }
    
    input[type=submit] {
      width: 30%;
      padding: 0;
      position: absolute;
      top: 0;
      left: 60%;
    }
}

#advanced-search {
    width: 10%;
    height: 35px;
    text-indent: -9999px;
    display: block;
    position: absolute;
    padding: 0;
    top: 0;
    left: 90%;
    font-weight: bold;
    line-height: 30px;
    background-color: #4b6569;
    @include has-layout-zoom();
    @include boxsizing(content-box);
    *zoom: expression( this.runtimeStyle['zoom'] = '1', this.innerHTML = '+');
    *text-indent: 0;

    
    &:link, &:hover, &:active, &:visited { color: #fff; }

    &:after {
        content: "+";
        display: block;
        text-indent: 0;
        text-align: center;
        margin-top: -1.75em;
    }
}

#advanced-form {
    display: none;
    background-color: #fff;
    overflow: auto;
    clear: both;
    z-index: 1001;
    position: absolute;
    top: 35px;
    left: 0;
    border: 1px solid #e7e7e7;
    width: 100%;
    padding: rhythm(1) 16px;
    text-align: left;
    @include boxsizing(border-box);
    @include box-shadow(rgba(0,0,0,.25) 0 5px 10px);
    
    #query-types *:first-child {
        margin-top: 0;
    }
    
    label {
        margin: 0;
        padding: 0;
    }
    
    p {
        @include leader(.5);
        @include trailer(.5);
    }
}

/* == Footer == */
footer {
    @include leader();
    padding: 0 16px;

    
    p {
        @include padding-trailer();
    }
    
    &:empty { display: none; }
}

/* == Content == */

    /* -- Structure -- */
    #content {
        clear:both;
        overflow:visible;
        padding: rhythm(0.25) 16px;
    }
    
    /* -- General -- */
    #home {

        #content div {
            
            .item {
                border-top: 1px solid $winter-light-gray;
                overflow: auto;
                
                &:first-of-type { 
                    border-top: 0; 
                    
                    h3 { margin-top: rhythm(.25); }
                }
                
                h3 { 
                    margin-top: rhythm(.5); 
                    padding-left: 16px;
                    padding-right: 16px;
                }
            }

            .items-list {
                margin-bottom: 0;
                .item-description {
                    margin: rhythm(.5) 16px 0;
                }
            }

            h3, h4, h5, h6,
            p, input, textarea {
                margin: rhythm(.5) 0;
            }

            h3 {
                margin-top: 0;
                font-weight: 300;
                text-transform: uppercase;
            }
            
            img { 
                margin: 0 16px rhythm(.5); 
                border: 1px solid $winter-light-gray;
                display: block;
                max-width: 100%;
                height: auto;
                @include boxsizing(border-box);
            }

            .show {
                display: block;
                text-transform: capitalize;
                &:before { content: "("; }
                &:after { content: "...)"; }
            }
            
            .view-items-link {
                margin: 0;
                padding: rhythm(.5) 16px 0;
                border-top: 1px solid $winter-light-gray;
            }
            
            .item-img {
                display: inline;
            }
            
            .item-description {
                margin-top: 0;
                padding-top: 0;
            }

        }
        
        #intro {
            @include trailer();
        }
        
        #featured-item p { clear: both; }
        
        #recent-items h2 { @include padding-trailer(.5); }

    }
        
    /* -- Navigation -- */
    nav {
        ul {
            list-style:none;
            margin:0 auto;
        }
        
        li {
            display:block;
            font-family: Georgia, Times, "Times New Roman", serif;
            font-style: italic;
            ul li {
                padding-left: 16px;
            }
        }
        
        a { display: inline-block; }
    }
    
    .top {
        padding: 0 16px;
        
        ul {
            display: none;
            padding: 0;
        }
        
        a {
            @include padding-leader(.5);
            @include padding-trailer(.5);
        }
        
        .mobile {
            margin: rhythm(1) auto;
            width: 100%;
        }
    }
    
    .navigation ul, ul.navigation { padding-left: 0; }
      

/* == Items Pages == */

.item {
    
    h3 { @include trailer(.5); }
    
    #itemfiles {
            
        #item-images > div { 
            width: 22.5%; 
            display: inline-block;
            margin-right: 3%;
            
            &:nth-of-type(4n) { margin-right: 0; }
        }
            
        a {
            border: 1px solid $winter-light-gray;
            display: inline-block;
            zoom: 1;
            *display: inline;
            min-width: 100%;
            overflow: visible;
            margin: 0;
            padding: 0;
            @include boxsizing(border-box);
            
            img {
                width: 100%;
                height: auto;
                vertical-align: bottom;
            }
        }
        .element-text {
            div:first-of-type {
                border-top: 1px solid $winter-light-gray;
            }
        }
    }
    
    #content #primary .element:last-of-type { padding-bottom: 0; }

    #primary {
        background-color: transparent;
        border-top: 0;
        padding: 0;
        @include box-shadow(transparent 0 0 0);
    }
    
}

.advanced-search #content {
    & > * > div {
        background-color: #fff;
        border-style: solid;
        border-width: 10px 3px 3px 3px;
        overflow: visible;
        padding: 0 0 rhythm(1) 0;
        @include box-shadow(rgba(0,0,0,.15) 0 0 10px);
        @include boxsizing(border-box);
        @include trailer(.75);
        
        &:last-of-type { padding: 0; }
        
        & > * {
            padding-left: 16px;
            padding-right: 16px;
        }
        
        input { @include trailer(); }
        
        div.label {
            @include trailer(.5);
            @include leader(.5);
        }
        
        & > div { 
            @include box-shadow(transparent 0 0 0);
            border-top: 0;
            margin-bottom: 0;
            padding: 0 16px;
            
            &:only-of-type, &:last-of-type { 
                margin-bottom: 0; 
            }
            
            div, h1, h2, h3, h4, h5, h6, p { 
                
                &:first-of-type { border-top: 0; }
            }
        }

        @include pie-clearfix();

    }

    .remove_search {
        @include trailer();
        margin-left: 0;
    }
    
    select { float: left; }
    
    button {
        display: inline-block;
        margin: 0 16px rhythm(1);
        
        &:last-of-type { margin-bottom: 0; }
    }
    
    .search-entry { 
        @include trailer(); 
        
        &:first-of-type { padding-bottom: 0; }
        
        &:last-of-type { @include trailer(); }
        
        &:only-of-type { @include trailer(0); }
    }
    
    #advanced-search-form { @include leader(.5); }
    
    #advanced-search-form > div:last-of-type {
        border: 0;
        background-color: transparent;
        float: left;
        @include pie-clearfix();
    }
    
    #submit_search_advanced {
        *height: 50px;
        margin-bottom: 0;
    }
    
    .view-items-link { padding: rhythm(.25) 16px rhythm(.75); }

}

/* == File Pages == */

.files {
    #content #primary { 
        @include block-reset(); 
        padding-top: 0;
        
        &:first-child { margin-top: 0; }
    }
    
    #content #sidebar h3 { padding-left: 0; }
    
    .download-file { 
        @include boxsizing(border-box);
