                text-align: center;
            }
            
            #exhibit-nav-next { @include trailer(); }
        }
        
        #exhibit-pages ul { margin-left: 0 !important; }
         
        .exhibit-page-nav, .exhibit-child-nav { 
            margin-left: 0; 
            padding-left: 0;
            
            .current { 
                font-weight: bold; 
                background-color: rgba(0, 0, 0, .1);
            }

        }
        
        .exhibit-page-nav {
        
            @include box-shadow(rgba(0,0,0,.15) 0 0 10px);
            @include trailer();

            a { padding: 0; }

            & > li:nth-child(2) {
                background-color: rgba(0,0,0,.1);
            }
            
            & > li:nth-child(3) {
                background-color: rgba(0,0,0,.2);
            }
            
            & > li:nth-child(4) {
                background-color: rgba(0,0,0,.3);
            }
            
            & > li {
                display: block;
                float: none;
                overflow: auto;
                margin-right: 0;
                
                &:first-of-type {
                    text-transform: uppercase;
                    margin-right: 0;
                    padding: rhythm(.25) 16px;
                    
                    a:after { content: ":"; }
                }
                
                ul {
                    padding-left: 0;
                    
                    li { 
                        display: inline-block; 
                        margin-right: 16px;
                        padding: rhythm(.25) 8px;
                        width: auto;

                        &:first-of-type {
                            padding-left: 16px;
                        }

                    }
                }
            }
        }
    }
}

#content .exhibit-item:empty { padding: 0; }

.gallery-full-left, .gallery-full-right, .gallery-thumbnails,
.gallery-thumbnails-text-bottom, .gallery-thumbnails-text-top, .image-list-left,
.image-list-left-thumbs, .image-list-right, .image-list-right-thumbs,
.text-full, .text-image-left, .text-image-right {
    padding: rhythm(1) 16px;
    @include leader();
    
    div:empty { display: none; }
}


/* == Simple Pages styles == */
.page #content #primary {
    background-color: #fff;
    border-style: solid;
    border-width: 10px 3px 3px 3px;
    overflow: visible;
    @include box-shadow(rgba(0,0,0,.15) 0 0 10px);
    @include boxsizing(border-box);
    @include padding-leader();
    @include padding-trailer();
    @include trailer(.75);
    @include prefix(.5);
    @include suffix(.5);
    *border-right: 1px solid #ccc;
    *border-left: 1px solid #ccc;
    *border-bottom: 1px solid #ccc;

    div {
        border-top: 0;
        @include box-shadow(transparent 0 0 0);
    }
    
    & > * { padding: 0; }
    
    & > ul, & > ol { padding-left: 0em; }
}

.simple-pages-navigation {
    list-style:none;
}
.simple-pages-navigation ul {
    display:none;
}
.simple-pages-navigation .current ul {
    display:block;
}

.simple-pages-navigation a {
    text-decoration:none;
    border-bottom:1px solid #ddd;
    display:block;
    font-weight:bold;
    padding:4px;
}

#simple-pages-breadcrumbs {
    font-weight:bold;
    @include trailer();
}



/*Night Styles*/
.night {

    body {
        background-color: $night-darkest;
        color: $night-text-on-darker;
    }
    
    h1 { color: $night-light; }
    
    #site-title a { color: $night-link; }

    input[type=submit], button, .button, #advanced-search { 
        background-color: $night-highlight; 
        color: $night-darker;
    }

    input[type=text], input[type=password], textarea { 
        background-color: $night-darker; 
        border: 1px solid $night-shadow;
        color: $night-text-on-darker;
    }

    a {
        &:link { color: $night-link; }
        &:visited { color: $night-visited; }
        &:hover, &:active { color: $night-active; }
    }

    header { background-color: $night-darkest; }
    
    nav.top { 
        background-color: $night-light; 

        li:hover a { color: $night-shadow; }
        
        a:link, a:visited  { color: $night-visited-nav; }
        a:active, a:hover  {  }
        
        ul li ul {
            background-color: $night-highlight;
            li {
                & > a:link, & > a:visited { color: $night-link; }
                & > a:active, & > a:hover, &:hover > a { color: $night-active; }
            }
        }
    }
    
    #intro { color: $night-light; }

    #content,
    #secondary-nav .current a,
    #secondary-nav a.current,
    .exhibit-section-nav .current a {
        background-color: $night-content;
    }
    
    #advanced-form {
        background-color: $night-darker;
        color: $night-text-on-darker;
        border-width: 3px 3px 3px 3px;
        border-style: solid;
        border-color: $night-shadow;
    }

    #home #content > div { 
        border-color: $night-highlight; 
        *border-width: 3px 3px 3px 3px;
        *border-style: solid;
        *border-color: #e8e8e8;
        *border-top-color: $night-highlight;
    }
    
    #home #content > div img, .item #itemfiles a,
    #home #content .item, #home #content div .view-items-link,
    .collections #content div .view-items-link { 
        border-color: $night-shadow; 
    }

    #content {
        h1 { color: $night-light; }
        
        h2 { border-color: $night-shadow; }

        & > div, 
        #primary > div,
        #sidebar > div,
        #advanced-search-form > div, 
        #exhibit-pages { 
            border-color: $night-highlight; 
            *border-width: 10px 3px 3px 3px;
            *border-style: solid;
            *border-color: #e8e8e8;
            *border-top-color: $night-highlight;
            background-color: $night-darker;
        }
        
        .pagination_list { 
            background-color: $night-darker; 
            
            a:link, a:visited { color: $night-highlight; }
            a:hover, a:active { color: #fff; }
        }

        .item-img { border-color: $night-shadow; }
        
        div.hTagcloud {
            border: 0;
            background-color: transparent;
            @include box-shadow(transparent 0 0 0);
        }
        
        #primary > div, #sidebar > div,
        .item-pagination li { background-color: $night-darker; }
    }
    
    #search-results th { 
        background-color: $night-highlight; 
        color: $night-light;
    }
    
    #search-results td { background-color: $night-darker; }

    #search-filters, #item-filters {
        li { background-color: $night-highlight; }
    }
    
    .page #content { 
        border-color: $night-highlight; 
        *border-width: 10px 1px 1px 1px;
        *border-style: solid;
        *border-color: #e8e8e8;
        *border-top-color: $night-highlight;
    }
    
    .page #content #primary { background-color: $night-darker; }

    footer {
        background-color: transparent;
    }

    footer p {
        color: $night-text-on-darker;
    }

    .exhibit-page-nav { background-color: $night-darker; }
    
    .exhibit-page-nav {
        a:link, a:visited { color: $night-darkest; }
        a:hover, a:active { color: #fff; }
    }
} 




