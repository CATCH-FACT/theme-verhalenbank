            border: 0;
            padding: 0;
            margin: 0;
            border-width: 0 0 0 0;
            border-color: green;
        }
        
        .pagination {
            border-width: 3px 3px 3px 3px;
            background-color: $verhalenbank_bg_white; 
            
            a:link, a:visited { 
